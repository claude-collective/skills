{
  "name": "@claude-collective/cc",
  "version": "0.1.0",
  "description": "CLI for managing Claude Collective skills, stacks, and agents",
  "type": "module",
  "main": "index.js",
  "bin": {
    "cc": "./dist/cli/index.js"
  },
  "scripts": {
    "build:agents": "node src/compile.mjs",
    "build": "tsup",
    "dev": "tsup --watch",
    "cli": "node dist/cli/index.js",
    "cli:dev": "bun src/cli/index.ts",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "cc:init": "bun src/cli/index.ts init",
    "cc:add": "bun src/cli/index.ts add",
    "cc:update": "bun src/cli/index.ts update",
    "cc:compile": "bun src/cli/index.ts compile",
    "cc:compile:fullstack": "bun src/cli/index.ts compile -s fullstack-react",
    "cc:compile:work": "bun src/cli/index.ts compile -s work-stack",
    "cc:compile:mobx": "bun src/cli/index.ts compile -s mobx-tailwind",
    "cc:validate": "bun src/cli/index.ts validate",
    "cc:config": "bun src/cli/index.ts config",
    "cc:list": "bun src/cli/index.ts list",
    "cc:switch": "bun src/cli/index.ts switch",
    "cc:dry-run": "bun src/cli/index.ts --dry-run",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "prepare": "husky"
  },
  "lint-staged": {
    "*.{ts,js,json,yaml,yml,md}": "prettier --write"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vincentbollaert/claude-subagents.git"
  },
  "author": "Vincent Bollaert",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/vincentbollaert/claude-subagents/issues"
  },
  "homepage": "https://github.com/vincentbollaert/claude-subagents#readme",
  "dependencies": {
    "@clack/prompts": "^0.11.0",
    "ajv": "^8.17.1",
    "ajv-formats": "^3.0.1",
    "commander": "^12.1.0",
    "fast-glob": "^3.3.0",
    "fs-extra": "^11.2.0",
    "giget": "^1.2.0",
    "liquidjs": "^10.24.0",
    "picocolors": "^1.1.0",
    "yaml": "^2.8.2"
  },
  "devDependencies": {
    "@types/fs-extra": "^11.0.0",
    "@types/node": "^22.0.0",
    "@vitest/coverage-v8": "^4.0.18",
    "husky": "^9.1.7",
    "lint-staged": "^15.2.0",
    "prettier": "^3.2.0",
    "tsup": "^8.0.0",
    "typescript": "^5.7.0",
    "vitest": "^4.0.18"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
